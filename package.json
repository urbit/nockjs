{
  "name": "@urbit/nockjs",
  "version": "1.4.0-dev4",
  "description": "Javascript urbit runtime",
  "main": "dist/index.js",
  "module": "dist/nockjs.esm.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/nockjs.esm.mjs"
      }
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "start": "dts watch",
    "build": "dts build && cp dist/index.d.ts dist/index.d.mts && cp dist/nockjs.esm.js dist/nockjs.esm.mjs",
    "test": "dts test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbit/nockjs.git"
  },
  "jest": {
    "verbose": true
  },
  "keywords": [
    "urbit",
    "nock",
    "js",
    "ts"
  ],
  "author": "frodwith, joemfb, iamwillkim, tomholford, polwex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/urbit/nockjs/issues"
  },
  "homepage": "https://github.com/urbit/nockjs#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "dts-cli": "^2.0.5",
    "jest": "^29.5.0",
    "typescript": "^5.0.4"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
